   "execution_count": 23,
   "metadata": {},
   "outputs": [],
   "source": [
    "def lesser_of_two_evens(a,b):\n",
    "    if a%2==0 and b%2==0:\n",
    "        if a>b:\n",
    "            return b\n",
    "            print(b)\n",
    "        else :\n",
    "            return a\n",
    "            print(a)\n",
    "    else:\n",
    "        if a>b:\n",
    "            return a\n",
    "            print (a)\n",
    "        else:\n",
    "            return b\n",
    "            print(b)\n",
    "    "
   ]
